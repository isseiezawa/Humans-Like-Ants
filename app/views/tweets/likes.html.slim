- set_meta_tags title: t('.title', post: @tweet.decorate.post_decorate)
h1.my-3.h3
  span
    = t('.title', post: @tweet.decorate.post_decorate)
.container-fluid
  table#liked-users-table.table.table-sm.table-hover.table-striped.table-bordered.border-secondary.shadow.text-center
    thead
      tr
        th
          = User.human_attribute_name(:name)
        th
          = User.human_attribute_name(:gender)
        th
          = User.human_attribute_name(:twitter_id)
        th
          = User.human_attribute_name(:avatar)
      tbody
        == render partial: 'liked_user', collection: @liked_users
